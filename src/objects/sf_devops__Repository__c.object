    <validationRules>
        <fullName>sf_devops__GitHub_Properties</fullName>
        <active>true</active>
        <description>Enforces that Owner and Repo are only set for GitHub repositories.</description>
        <errorConditionFormula>AND(
  ISNEW(),
   NOT(ISPICKVAL( sf_devops__Provider__c , &quot;GitHub&quot;)),
  NOT(AND (
    ISBLANK( sf_devops__GitHub_Owner__c),
    ISBLANK( sf_devops__GitHub_Repo__c)
  ))
)</errorConditionFormula>
        <errorMessage>GitHub_Repo and GitHub_Owner can only be set for GitHub repositories.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__No_Property_Changes</fullName>
        <active>true</active>
        <description>Enforces that certain fields cannot be changed.</description>
        <errorConditionFormula>AND(
  OR (
    ISCHANGED( sf_devops__Provider__c ) ,
    ISCHANGED(  sf_devops__GitHub_Owner__c  ) ,
    ISCHANGED(  sf_devops__GitHub_Repo__c  ) 
  )
)</errorConditionFormula>
        <errorMessage>This field cannot be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Require_GitHub_Properties</fullName>
        <active>true</active>
        <description>Enforces that Owner and Repo are set for GitHub repositories.</description>
        <errorConditionFormula>AND(
  ISNEW(),
   ISPICKVAL( sf_devops__Provider__c , &quot;GitHub&quot;),
  OR (
    ISBLANK( sf_devops__GitHub_Owner__c),
    ISBLANK( sf_devops__GitHub_Repo__c)
  )
)</errorConditionFormula>
        <errorMessage>Repo and Owner are required for GitHub repositories.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
