    <validationRules>
        <fullName>sf_devops__No_Platform_Repository_Change</fullName>
        <active>true</active>
        <description>Enforces that Platform_Repository__c cannot be changed.</description>
        <errorConditionFormula>ISCHANGED(  sf_devops__Platform_Repository__c  )</errorConditionFormula>
        <errorMessage>Platform_Repository__c cannot be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Unique_Name</fullName>
        <active>true</active>
        <description>Enforces that all projects have a unique name</description>
        <errorConditionFormula>AND(
  OR(
    ISNEW(),
    ISCHANGED( Name )
  ),
  NOT(
    ISBLANK (
      VLOOKUP( $ObjectType.sf_devops__Project__c.Fields.Name, 
               $ObjectType.sf_devops__Project__c.Fields.Name,
               Name )
     )
  )
)</errorConditionFormula>
        <errorMessage>Projects require a unique name.</errorMessage>
    </validationRules>
