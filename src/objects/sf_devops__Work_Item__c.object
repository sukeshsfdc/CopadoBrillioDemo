    <validationRules>
        <fullName>sf_devops__Approved_By_Without_Approved</fullName>
        <active>true</active>
        <description>Enforce Approved is set if Approved_By is set.</description>
        <errorConditionFormula>AND(
OR(
ISNEW(),
ISCHANGED(sf_devops__Development_Approved__c),
ISCHANGED(sf_devops__Development_Approved_By__c)
),
NOT(sf_devops__Development_Approved__c),
NOT(ISBLANK( sf_devops__Development_Approved_By__c  ))

)</errorConditionFormula>
        <errorMessage>A Work_Item can not have Development_Approved_By set if it is not Development_Approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Approved_Without_Approved_By</fullName>
        <active>true</active>
        <description>Enforce ApprovedBy is set if this item is approved.</description>
        <errorConditionFormula>AND(
OR(
ISNEW(),
ISCHANGED(sf_devops__Development_Approved__c),
ISCHANGED(sf_devops__Development_Approved_By__c)
),
sf_devops__Development_Approved__c,
ISBLANK( sf_devops__Development_Approved_By__c  )

)</errorConditionFormula>
        <errorMessage>You must set Development_Approved_By when this Work Item is approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Approved_Without_Review</fullName>
        <active>true</active>
        <description>Enforce that if Approved is set then this item has a review.</description>
        <errorConditionFormula>AND(
OR(
ISNEW(),
ISCHANGED(sf_devops__Development_Approved__c),
ISCHANGED(sf_devops__Review_Remote_Reference__c),
ISCHANGED(sf_devops__Promoted__c)
),
sf_devops__Development_Approved__c,
ISBLANK( sf_devops__Review_Remote_Reference__c  ),
NOT( sf_devops__Promoted__c)

)</errorConditionFormula>
        <errorMessage>A Work Item cannot be approved without a review.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Branch_Without_Remote_Reference</fullName>
        <active>false</active>
        <description>This rule is no longer enforced</description>
        <errorConditionFormula>FALSE</errorConditionFormula>
        <errorMessage>Deprecated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Change_Bundle_Project</fullName>
        <active>true</active>
        <description>Enforce we belong to the same project as our change bundle.</description>
        <errorConditionFormula>AND(
OR(
ISNEW(),
ISCHANGED(sf_devops__Change_Bundle__c)
),
NOT(ISBLANK(sf_devops__Change_Bundle__c)),
sf_devops__Project__c &lt;&gt; sf_devops__Change_Bundle__r.sf_devops__Project__c

)</errorConditionFormula>
        <errorMessage>A Work Item must belong to the same project as its Change Bundle.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Concluded_values</fullName>
        <active>true</active>
        <description>Verifies that Concluded_c is assigned with a possible value.</description>
        <errorConditionFormula>NOT(OR( sf_devops__Concluded__c = &quot;CLOSED&quot;, sf_devops__Concluded__c = &quot;NEVERED&quot;, ISBLANK(sf_devops__Concluded__c)))</errorConditionFormula>
        <errorMessage>Possible values for Concluded_c are : CLOSED, NEVERED, or empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Disable_Concluded</fullName>
        <active>true</active>
        <description>A Work Item cannot have its Concluded__c field changed after being set to NEVER or CLOSED.</description>
        <errorConditionFormula>AND(ISCHANGED(sf_devops__Concluded__c),NOT(ISBLANK(PRIORVALUE(sf_devops__Concluded__c))))</errorConditionFormula>
        <errorMessage>A Work Item cannot have its Concluded__c field changed once set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Disable_Promoted</fullName>
        <active>true</active>
        <description>A Work Item cannot have its Promoted__c field disabled once set.</description>
        <errorConditionFormula>AND(PRIORVALUE(sf_devops__Promoted__c), ISCHANGED(sf_devops__Promoted__c))</errorConditionFormula>
        <errorMessage>A Work Item cannot have its Promoted__c field disabled once set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Environment_Project</fullName>
        <active>true</active>
        <description>Enforce we belong to the same project as our environment.</description>
        <errorConditionFormula>AND(
OR(
ISNEW(),
ISCHANGED(sf_devops__Environment__c)
),
NOT(ISBLANK(sf_devops__Environment__c)),
sf_devops__Project__c &lt;&gt; sf_devops__Environment__r.sf_devops__Project__c

)</errorConditionFormula>
        <errorMessage>A Work Item must belong to the same project as its Environment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Environment_Requires_Tracking</fullName>
        <active>true</active>
        <description>Enforce we only reference environments with source tracking.</description>
        <errorConditionFormula>AND(
OR(
ISNEW(),
ISCHANGED(sf_devops__Environment__c)
),
NOT(ISBLANK( sf_devops__Environment__c  )),
NOT(sf_devops__Environment__r.sf_devops__Can_Track_Changes__c)

)</errorConditionFormula>
        <errorMessage>Cannot reference environment that does not support change tracking.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__PromotedFromEnvironment_Changed</fullName>
        <active>true</active>
        <description>A Work Item cannot have its Promoted_From_Environment__c field changed once set.</description>
        <errorConditionFormula>AND(NOT(ISBLANK(PRIORVALUE(sf_devops__Promoted_From_Environment__c))), ISCHANGED(sf_devops__Promoted_From_Environment__c))</errorConditionFormula>
        <errorMessage>A Work Item cannot have its Promoted_From_Environment__c field changed once set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__PromotedFromEnvironment_Set</fullName>
        <active>true</active>
        <description>A Work Item cannot have its Promoted_From_Environment__c field set with a value different to the value of Environment__c and it can only be set when Environment__c is getting set to null.</description>
        <errorConditionFormula>AND(
    ISCHANGED(sf_devops__Promoted_From_Environment__c),
    OR(
      NOT(ISCHANGED(sf_devops__Environment__c)),
      NOT(ISBLANK(sf_devops__Environment__c)),
      sf_devops__Promoted_From_Environment__c &lt;&gt; PRIORVALUE(sf_devops__Environment__c)
    )
)</errorConditionFormula>
        <errorMessage>A Work Item cannot have its Promoted_From_Environment__c field set with a value different to the value of Environment__c and it can only be set when Environment__c is getting set to null.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Promoted_And_Installed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(
    ISNEW(),
    ISCHANGED( sf_devops__Promoted__c ),
    ISCHANGED(  sf_devops__Change_Bundle__c  )
 ),
 NOT(sf_devops__Promoted__c), 
 NOT (ISBLANK(sf_devops__Change_Bundle__c))
)</errorConditionFormula>
        <errorMessage>A Work_Item cannot be a member of a Change_Bundle if it has never been promoted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Promoted_On_Create</fullName>
        <active>true</active>
        <description>Enforce we cannot create a work item with Promoted__c set.</description>
        <errorConditionFormula>AND(
ISNEW(),
sf_devops__Promoted__c

)</errorConditionFormula>
        <errorMessage>You cannot create a Work_Item with the Promoted flag set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Promoted_Without_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 OR(
    ISNEW(),
    ISCHANGED(sf_devops__Development_Approved__c),
    ISCHANGED(sf_devops__Promoted__c)
 ), 
   NOT (sf_devops__Development_Approved__c) ,
   sf_devops__Promoted__c 
 
 )</errorConditionFormula>
        <errorMessage>A WorkItem cannot be promoted if it is not Approved.</errorMessage>
    </validationRules>
    <validationRules>
