    <validationRules>
        <fullName>sf_devops__Branch_Without_Remote_Reference</fullName>
        <active>false</active>
        <description>This rule is no longer enforced</description>
        <errorConditionFormula>FALSE</errorConditionFormula>
        <errorMessage>Deprecated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__No_Changes_For_Active_Pipelines</fullName>
        <active>true</active>
        <description>Do not allow changes to a stage on an active pipeline</description>
        <errorConditionFormula>AND(
 OR (
    AND(
      ISCHANGED( sf_devops__Branch__c  ), 
      NOT(ISBLANK(PRIORVALUE(sf_devops__Branch__c)))
    ) ,
    ISCHANGED(  sf_devops__Environment__c  ) ,
    ISCHANGED(  sf_devops__Next_Stage__c  ) ,
    ISCHANGED(   sf_devops__Versioned__c   )
  ),
 sf_devops__Pipeline__r.sf_devops__Activated__c )</errorConditionFormula>
        <errorMessage>You cannot modify a Pipeline_Stage on an activated Pipeline.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__No_New_Stages_For_Active_Pipelines</fullName>
        <active>true</active>
        <description>Do not allow new stages to be added to activated pipelines</description>
        <errorConditionFormula>AND( ISNEW(),
 sf_devops__Pipeline__r.sf_devops__Activated__c )</errorConditionFormula>
        <errorDisplayField>sf_devops__Pipeline__c</errorDisplayField>
        <errorMessage>You cannot add a Pipeline_Stage to an activated Pipeline.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Promote_Review_Remote_Ref_Required</fullName>
