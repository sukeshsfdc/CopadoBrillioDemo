    <validationRules>
        <fullName>sf_devops__No_Promotion_to_Inactive_Pipeline</fullName>
        <active>true</active>
        <description>You cannot promote a work item to a stage of an inactive pipeline</description>
        <errorConditionFormula>AND (
      ISNEW(),
   NOT( sf_devops__Pipeline_Stage__r.sf_devops__Pipeline__r.sf_devops__Activated__c )
)</errorConditionFormula>
        <errorMessage>You cannot promote a Work_Item to an inactive Pipeline.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Promoted_And_Installed</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
      ISNEW(),
 NOT (ISBLANK(sf_devops__Work_Item__r.sf_devops__Change_Bundle__c))
)</errorConditionFormula>
        <errorMessage>A Work_Item that is a member of a Change_Bundle can not be promoted into a Pipeline_Stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Promoted_To_Versioned</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
      ISNEW(),
      sf_devops__Pipeline_Stage__r.sf_devops__Versioned__c 
)</errorConditionFormula>
        <errorMessage>A Work_Item cannot be promoted into a versioned Pipeline_Stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Promoted_Without_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    ISNEW(),
   NOT (sf_devops__Work_Item__r.sf_devops__Development_Approved__c) 
 
 )</errorConditionFormula>
        <errorMessage>A WorkItem cannot be promoted if it is not Approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Promoted_Wrong_Project</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    ISNEW(),
    sf_devops__Pipeline_Stage__r.sf_devops__Pipeline__r.sf_devops__Project__c  &lt;&gt;  sf_devops__Work_Item__r.sf_devops__Project__c 

)</errorConditionFormula>
        <errorMessage>A WorkItem can only be promoted to a stage within the same Project.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
